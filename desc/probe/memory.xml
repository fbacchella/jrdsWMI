<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>PerfOS_Memory</name>
    <probeName>memory</probeName>
    <probeClass>jrds.wmi.WMIProbe</probeClass>
    <specific name="wbemClass">Win32_PerfRawData_PerfOS_Memory</specific>
    <ds>
        <dsName>AvailableBytes</dsName>
        <dsType>gauge</dsType>
        <collect>AvailableBytes#PERF_COUNTER_LARGE_RAWCOUNT</collect>
    </ds>
    <ds>
        <dsName>CacheBytes</dsName>
        <dsType>gauge</dsType>
        <collect>CacheBytes#PERF_COUNTER_LARGE_RAWCOUNT</collect>
    </ds>
    <ds>
        <dsName>CacheFaults</dsName>
        <dsType>counter</dsType>
        <collect>CacheFaultsPerSec#PERF_COUNTER_RAWCOUNT</collect>
    </ds>
    <ds>
        <dsName>CommitLimit</dsName>
        <dsType>gauge</dsType>
        <collect>CommitLimit#PERF_COUNTER_LARGE_RAWCOUNT</collect>
    </ds>
    <ds>
        <dsName>CommittedBytes</dsName>
        <dsType>gauge</dsType>
        <collect>CommittedBytes#PERF_COUNTER_LARGE_RAWCOUNT</collect>
    </ds>
    <ds>
        <dsName>DemandZeroFaults</dsName>
