        <collect>SystemCodeTotalBytes#PERF_COUNTER_LARGE_RAWCOUNT</collect>
    </ds>
    <ds>
        <dsName>SystemDriverRsdB</dsName>
        <dsType>gauge</dsType>
        <collect>SystemDriverResidentBytes#PERF_COUNTER_LARGE_RAWCOUNT</collect>
    </ds>
    <ds>
        <dsName>SystemDriverTotalB</dsName>
        <dsType>gauge</dsType>
        <collect>SystemDriverTotalBytes#PERF_COUNTER_LARGE_RAWCOUNT</collect>
