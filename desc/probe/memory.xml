    </ds>
    <ds>
        <dsName>PageFaultsPerSec</dsName>
        <dsType>counter</dsType>
        <collect>PageFaultsPerSec#PERF_COUNTER_COUNTER</collect>
    </ds>
    <ds>
        <dsName>PageReadsPerSec</dsName>
        <dsType>counter</dsType>
        <collect>PageReadsPerSec#PERF_COUNTER_COUNTER</collect>
    </ds>
    <ds>
        <dsName>PagesInputPerSec</dsName>
        <dsType>counter</dsType>
        <collect>PagesInputPerSec#PERF_COUNTER_COUNTER</collect>
    </ds>
    <ds>
        <dsName>PagesOutputPerSec</dsName>
        <dsType>counter</dsType>
        <collect>PagesOutputPerSec#PERF_COUNTER_COUNTER</collect>
    </ds>
    <ds>
        <dsName>PagesPerSec</dsName>
        <dsType>counter</dsType>
        <collect>PagesPerSec#PERF_COUNTER_COUNTER</collect>
    </ds>
    <ds>
        <dsName>PageWritesPerSec</dsName>
        <dsType>counter</dsType>
        <collect>PageWritesPerSec#PERF_COUNTER_COUNTER</collect>
    </ds>
    <ds>
        <dsName>PoolNonpagedAllocs</dsName>
        <dsType>gauge</dsType>
        <collect>PoolNonpagedAllocs#PERF_COUNTER_RAWCOUNT</collect>
    </ds>
    <ds>
        <dsName>PoolNonpagedBytes</dsName>
        <dsType>gauge</dsType>
    </ds>
    <ds>
        <dsName>PoolNonpagedBytes</dsName>
        <dsType>gauge</dsType>
        <collect>PoolNonpagedAllocs#PERF_COUNTER_LARGE_RAWCOUNT</collect>
    </ds>
    <ds>
        <dsName>PoolPagedBytes</dsName>
        <dsType>gauge</dsType>
        <collect>PoolPagedBytes#PERF_COUNTER_LARGE_RAWCOUNT</collect>
    </ds>
    <ds>
        <dsName>PoolPagedResidentB</dsName>
        <dsType>gauge</dsType>
        <collect>PoolPagedResidentBytes#PERF_COUNTER_LARGE_RAWCOUNT</collect>
